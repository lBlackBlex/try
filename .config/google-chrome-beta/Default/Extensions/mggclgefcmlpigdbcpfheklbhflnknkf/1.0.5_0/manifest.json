{
   "content_scripts": [ {
      "css": [ "wbs.css" ],
      "js": [ "content.js" ],
      "matches": [ "*://alpha.wallhaven.cc/wallpaper/*", "*://alpha.wallhaven.cc/tag/*", "*://alpha.wallhaven.cc/*" ]
   } ],
   "description": "Allows you to download the wallpapers on Wallhaven directly from the search/tag/random/toplist pages",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Wallhaven Direct Downloader",
   "permissions": [ "*://alpha.wallhaven.cc/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.5",
   "web_accessible_resources": [ "wallbase.eot", "wallbase.ttf", "wallbase.svg", "wallbase.woff" ]
}
